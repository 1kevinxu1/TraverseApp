<div class="trip-details">
  <div class="row">
    <div class="city-date pull-left">
      <h4><%= trip.get("from_city") %></h4>
      <%= trip.get("start_date_string") %>
    </div>
    <div class="city-date pull-right">
      <h4><%= trip.get("to_city") %></h4>
      <%= trip.get("end_date_string") %>
    </div>
  </div>
</div>
