PENDING REQUESTS <br>
  <% if (user) { %>
    <%= user.get("name") %>
    <%= user.get("age") %>
    <img src="<%=user.get("image_url")%>" height="150px" width="150px">
    <div id="accept" class="btn btn-success">Accept</div>
    <div id="decline" class="btn btn-danger">Decline</div>
  <% } else { %>
    <div class="placeholder-2">NO PENDING REQUESTS</div>
  <% } %>
